{
  "name": "jike-sdk",
  "version": "0.23.2",
  "packageManager": "pnpm@8.6.4",
  "description": "Jike Ⓙ SDK for Node.js",
  "type": "module",
  "keywords": [
    "jike",
    "sdk"
  ],
  "license": "AGPL-3.0",
  "homepage": "https://github.com/open-jike/jike-sdk#readme",
  "bugs": {
    "url": "https://github.com/open-jike/jike-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-jike/jike-sdk.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/node/index.cjs",
      "import": "./dist/index.js"
    },
    "./polyfill": {
      "types": "./dist/index.d.ts",
      "require": "./dist/node/index.cjs",
      "import": "./dist/node/index.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "<=4.9": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "tsx playground/main.ts",
    "build": "tsup && pnpm run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration",
    "docs:build": "typedoc",
    "docs:preview": "serve docs",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "test": "vitest",
    "test:deno": "cd tests/deno && deno run --allow-net --import-map=import_map.json --allow-env index.ts",
    "release": "bumpp && npm publish",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "ky": "^0.33.3",
    "undici": "^5.22.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@rollup/plugin-inject": "^5.0.3",
    "@sxzz/eslint-config": "^3.1.0",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.16.18",
    "@types/sharp": "^0.31.1",
    "@vitest/ui": "^0.32.2",
    "bumpp": "^9.1.1",
    "esbuild": "^0.18.8",
    "eslint": "^8.43.0",
    "eslint-define-config": "^1.21.0",
    "md5": "^2.3.0",
    "rollup": "^3.25.2",
    "serve": "^14.2.0",
    "sharp": "^0.32.1",
    "tsup": "^7.1.0",
    "tsx": "^3.12.7",
    "typedoc": "0.24.8",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
