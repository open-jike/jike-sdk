{
  "name": "jike-sdk",
  "version": "0.22.0",
  "packageManager": "pnpm@7.29.3",
  "description": "Jike Ⓙ SDK for Node.js",
  "type": "module",
  "keywords": [
    "jike",
    "sdk"
  ],
  "license": "AGPL-3.0",
  "homepage": "https://github.com/open-jike/jike-sdk#readme",
  "bugs": {
    "url": "https://github.com/open-jike/jike-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-jike/jike-sdk.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "module": "dist/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/node/index.cjs",
      "import": "./dist/node/index.js"
    },
    "./index": {
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "tsx playground/main.ts",
    "build": "pnpm run build:lib && pnpm run build:types",
    "build:lib": "MODE=modern tsup && MODE=node tsup",
    "build:types": "tsc --emitDeclarationOnly --declaration",
    "docs:build": "typedoc",
    "docs:preview": "serve docs",
    "lint": "eslint . --ext .js,.ts,.yml,.yaml,.md,.json,.json5",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest",
    "test:deno": "cd tests/deno && deno run --allow-net --import-map=import_map.json --allow-env index.ts",
    "release": "bumpp && npm publish",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "eventemitter3": "^5.0.0",
    "ky": "^0.33.3",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@sxzz/eslint-config-prettier": "^2.4.5",
    "@sxzz/eslint-config-ts": "^2.4.5",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.15.3",
    "@types/sharp": "^0.31.1",
    "@vitest/ui": "^0.29.3",
    "bumpp": "^9.0.0",
    "compare-versions": "^5.0.3",
    "esbuild": "^0.17.12",
    "eslint": "^8.36.0",
    "eslint-define-config": "^1.17.0",
    "md5": "^2.3.0",
    "rollup": "^3.19.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "serve": "^14.2.0",
    "sharp": "^0.31.3",
    "tsup": "^6.6.3",
    "tsx": "^3.12.5",
    "typedoc": "0.24.0-beta.3",
    "typescript": "^4.9.5",
    "vitest": "^0.29.3"
  },
  "engines": {
    "node": ">=14.19.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "typescript": "4"
      }
    }
  }
}
