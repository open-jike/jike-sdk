{
  "name": "jike-sdk",
  "version": "0.21.0",
  "packageManager": "pnpm@7.26.2",
  "description": "Jike Ⓙ SDK for Node.js",
  "type": "module",
  "keywords": [
    "jike",
    "sdk"
  ],
  "license": "AGPL-3.0",
  "homepage": "https://github.com/open-jike/jike-sdk#readme",
  "bugs": {
    "url": "https://github.com/open-jike/jike-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-jike/jike-sdk.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "module": "dist/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/node/index.cjs",
      "import": "./dist/node/index.js"
    },
    "./index": {
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "tsx playground/main.ts",
    "build": "pnpm run build:lib && pnpm run build:types",
    "build:lib": "MODE=modern tsup && MODE=node tsup",
    "build:types": "tsc --emitDeclarationOnly --declaration",
    "docs:build": "typedoc",
    "docs:preview": "serve docs",
    "lint": "eslint . --ext .js,.ts,.yml,.yaml,.md,.json,.json5",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest",
    "test:deno": "cd tests/deno && deno run --allow-net --import-map=import_map.json --allow-env index.ts",
    "release": "bumpp && npm publish",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "eventemitter3": "^5.0.0",
    "ky": "^0.33.2",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@sxzz/eslint-config-prettier": "^2.4.4",
    "@sxzz/eslint-config-ts": "^2.4.4",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.11.18",
    "@types/sharp": "^0.31.1",
    "@vitest/ui": "^0.28.3",
    "bumpp": "^8.2.1",
    "compare-versions": "^5.0.3",
    "dayjs": "^1.11.7",
    "esbuild": "^0.17.5",
    "eslint": "^8.33.0",
    "eslint-define-config": "^1.14.0",
    "md5": "^2.3.0",
    "rollup": "^3.12.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "serve": "^14.2.0",
    "sharp": "^0.31.3",
    "tsup": "^6.5.0",
    "tsx": "^3.12.2",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4",
    "vitest": "^0.28.3"
  },
  "engines": {
    "node": ">=14.19.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "typescript": "4"
      }
    }
  }
}
